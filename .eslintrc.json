{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "rules": {
        "no-console": "error",
        "no-unused-vars": "error",
        "prettier/prettier": "error",
        "@typescript-eslint/naming-convention":[
            "error",
            {
                "selector": "variable",
                "format": ["camelCase"]
            }
        ]
    }
}